#include <stdio.h>
int main()
{ 
    int n,s=0,rev=0,t,r;
    
    printf("enternum:");
    scanf("%d",&n);
    t=n;
    while(n!=0)
    {
        r=n%10;
        rev=rev*10+r;
        n=n/10;
    }
    
    if(rev==t)
    printf("palindrome!");
    
    else
    printf("not palindrome!");
    
    return 0;
    
}
