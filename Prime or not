//check wether the num is primre or not
#include<stdio.h>
int num,i,f;
int main(){
    printf("enter value);
    scanf("%d",&num);
    f=0;
    for(i=2;i<num;i++)
    {if(num%i==0)
      f=1;
      break;
     
    
    }
   
    if(f==1)
    printf("composite");
    
    else
    printf("prime");
    
    
    return 0;
}    
    
    
    
  
